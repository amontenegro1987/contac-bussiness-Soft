public enum RoundingMode {

    ROUNDING_MODE(BigDecimal.ROUND_HALF_EVEN), //Rounding mode
    ROUNDING_SCALE(19), //Rounding scale
    ROUNDING_PRECISION(4); //Rounding precision

    private int value;

    RoundingMode(int value) {
        this.value = value;
    }

    public int getValue() {
        return value;
    }
}
