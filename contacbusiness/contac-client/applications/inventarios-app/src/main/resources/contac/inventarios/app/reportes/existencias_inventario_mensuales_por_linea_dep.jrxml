	<queryString>
		<![CDATA[SELECT
    LINEA.c_descripcion,
	SUM(IF(month(MOV_INVENTARIO.d_ctime) = 1,ABS(MOV_INVENTARIO.N_CANTIDAD * 		MOV_INVENTARIO.N_AFECTACION),0)) AS "ENERO",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 2,ABS(MOV_INVENTARIO.N_CANTIDAD * 		MOV_INVENTARIO.N_AFECTACION),0)) AS "FEBRERO",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 3,ABS(MOV_INVENTARIO.N_CANTIDAD * 		MOV_INVENTARIO.N_AFECTACION),0)) AS "MARZO",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 4,ABS(MOV_INVENTARIO.N_CANTIDAD *     		MOV_INVENTARIO.N_AFECTACION),0)) AS "ABRIL",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 5,ABS(MOV_INVENTARIO.N_CANTIDAD * 		MOV_INVENTARIO.N_AFECTACION),0)) AS "MAYO",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 6,ABS(MOV_INVENTARIO.N_CANTIDAD * 		MOV_INVENTARIO.N_AFECTACION),0)) AS "JUNIO",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 7,ABS(MOV_INVENTARIO.N_CANTIDAD * 		MOV_INVENTARIO.N_AFECTACION),0)) AS "JULIO",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 8,ABS(MOV_INVENTARIO.N_CANTIDAD *  		MOV_INVENTARIO.N_AFECTACION),0)) AS "AGOSTO",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 9,ABS(MOV_INVENTARIO.N_CANTIDAD * 		MOV_INVENTARIO.N_AFECTACION),0)) AS "SEPTIEMBRE",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 10,ABS(MOV_INVENTARIO.N_CANTIDAD *    		MOV_INVENTARIO.N_AFECTACION),0)) AS "OCTUBRE",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 11,ABS(MOV_INVENTARIO.N_CANTIDAD * 		MOV_INVENTARIO.N_AFECTACION),0)) AS "NOVIEMBRE",
          SUM(IF(month(MOV_INVENTARIO.d_ctime) = 12,ABS(MOV_INVENTARIO.N_CANTIDAD * 		MOV_INVENTARIO.N_AFECTACION),0)) AS "DICIEMBRE",
	UNIDADMED.C_NOMBRE AS UNIDAD_MEDIDA,
          PROD.N_ID_COMPANIA  as "idcompania"
    FROM 	INV_PRODUCTO AS PROD LEFT JOIN INV_MOV_INVENTARIO MOV_INVENTARIO ON PROD.N_ID = 		MOV_INVENTARIO.N_ID_PRODUCTO
	LEFT JOIN INV_ESTADO_MOVIMIENTO ESTADO ON MOV_INVENTARIO.N_ID_ESTADO = estado.N_ID
	LEFT JOIN INV_ALMACEN ALMACEN_MOV ON MOV_INVENTARIO.N_ID_ALMACEN = ALMACEN_MOV.N_ID
	LEFT JOIN INV_PRODUCTOEXISTENCIA PROD_EXISTENCIA ON PROD.N_ID = 			PROD_EXISTENCIA.N_ID_PRODUCTO
	LEFT JOIN INV_ALMACEN ALMACEN_PROD ON PROD_EXISTENCIA.N_ID_ALMACEN = 			ALMACEN_PROD.N_ID
	INNER JOIN INV_UNIDADMEDIDA UNIDADMED ON PROD.N_ID_UNIDADMEDIDA = UNIDADMED.N_ID
	INNER JOIN INV_LINEA LINEA ON PROD.N_ID_LINEA = LINEA.N_ID
	INNER JOIN CMP_PROVEEDOR PROVEEDOR ON PROD.N_ID_PROVEEDOR = PROVEEDOR.N_ID
	AND ALMACEN_MOV.N_ID > 0
	AND ALMACEN_MOV.N_ESTATUS <> 2
	AND year(MOV_INVENTARIO.d_ctime) =  (Select Year(CURDATE()))
	GROUP BY LINEA.c_descripcion;]]>
	</queryString>
